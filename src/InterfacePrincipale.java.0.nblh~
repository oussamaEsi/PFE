
import java.io.*;
import java.math.BigInteger;
import java.util.logging.Level;
import java.util.logging.Logger;
import javax.swing.ImageIcon;
import javax.swing.JFileChooser;
import javax.swing.JOptionPane;
import javax.swing.SwingUtilities;
import javax.swing.UIManager;
import javax.swing.UnsupportedLookAndFeelException;

public class InterfacePrincipale extends javax.swing.JFrame
{private static class ChiffrementSymetrique implements Runnable
{public void run()
{byte[] blocClair=new byte[1023],blocChiffre = null,extensionChiffre;
BufferedInputStream in;
BufferedOutputStream out = null;
int nread = 0;
int progression;

//Remarque : Initialiser la taille de blocClair avec une taille qui corréspond à l'algorithme choisi.
//  Initialisation de la valeur de progression de la bar de progression.
progression=blocClair.length;
// Instanciation de l'algorithme de chiffrement symétrique choisi.
int algo=jComboBox3.getSelectedIndex();
  try
  {algorithme = new AlgorithmesChiffrementSymetrique(algo);
  }
  catch (Exception ex)
  {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
  }
// Récupérer la clé de chiffrement/
String cle=jTextField3.getText().trim();
// Récupérer le chemin d'accés au fichier à chiffrer
String urlFichier=jTextField4.getText().trim();
// Création de fichier à chiffrer.
File fichier=new File(urlFichier);
  try
  {// Création d'un flux pour la lécture de fichier.
  in = new BufferedInputStream(new FileInputStream(urlFichier));
  }
  catch (FileNotFoundException ex)
  {JOptionPane jop=new JOptionPane();
  jop.showMessageDialog(null,"Fichier non trouvé.","Erreur",JOptionPane.ERROR_MESSAGE);
  jTextField4.setText(null);
  return;
  }
// Récupérer l'extension de fichier à chiffrer.
String extension = urlFichier.substring(urlFichier.lastIndexOf("."), urlFichier.length());
  if(extension.equals(".crp"))
  {JOptionPane jop=new JOptionPane();
  jop.showMessageDialog(null,"Fichier déja chiffré.","Erreur",JOptionPane.ERROR_MESSAGE);
  jTextField4.setText(null);
  return;
  }
  try
  {// Chiffrement de l'éxtension.
  extensionChiffre=algorithme.chiffrer(algo,extension.getBytes(),cle);
  }
  catch (Exception ex)
  {JOptionPane jop=new JOptionPane();
  jop.showMessageDialog(null,"La clé secrète est invalide.","Erreur",JOptionPane.ERROR_MESSAGE);
  jTextField3.setText(null);
  return;
  }
// Construire le chemin d'accés au nouveau fichier (chiffré).
urlFichier = urlFichier.substring(0, urlFichier.lastIndexOf("."));
// Extension de nouveau fichier (chiffré).
urlFichier+=".crp";
  try
  {// Création d'un flux pour l'écriture dans le nouveau fichier (chiffré).
  out =new BufferedOutputStream(new FileOutputStream(urlFichier));
  }
  catch (FileNotFoundException ex)
  {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
  }
  try
  {// Ecriture dans le nouveau fichier l'éxtension chiffrée.
  out.write(extensionChiffre);
  }
  catch (IOException ex)
  {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
  }
// Initialisation de la taille maximale de jProgressBar1 avec la taille de fichier à chiffrer en octets.
jProgressBar1.setMaximum((int)fichier.length());
jProgressBar1.setMinimum(0);
  try
  {// lécture d'un bloc d'octets clair de taille 1023 octet.
  nread = in.read(blocClair);
  }
  catch (IOException ex)
  {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
  }
arretChiffrement=false;
  while ((nread!=-1)&&(!arretChiffrement))
  {  try
     {// Chiffrement de bloc clair d'octets.
     blocChiffre = algorithme.chiffrer(algo,blocClair, cle);
     }
     catch (Exception ex)
     {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
     }
     try
     {// Ecriture de bloc d'octets chiffre dans le nouveau fichier.
     out.write(blocChiffre);
     }
     catch (IOException ex)
     {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
     }
   // Progression de la bar de progression.
   jProgressBar1.setValue(jProgressBar1.getValue()+progression);
     try
     {// Lecture d'un autre bloc clair.
     nread = in.read(blocClair);
     }
     catch (IOException ex)
     {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
     }
  }
  try
  {// Ferméture des flux.
  in.close();
  out.close();
  }
  catch (IOException ex)
  {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
  }
}
}
