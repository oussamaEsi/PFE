    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
      try
      {ClesSecretes c = new ClesSecretes(jComboBox1.getSelectedIndex());
      jTextField1.setText(c.getCle());
      }
      catch (Exception ex)
      {System.out.println("Erreur création du clé.");
      }
    }//GEN-LAST:event_jButton1ActionPerformed

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
      if((jTextField1.getText()!=null)&&(jTextField1.getText().length()!=0))
      {JFileChooser boiteOuvrir = new JFileChooser();
      boiteOuvrir.setCurrentDirectory(new File("fichiers" + File.separator));
      boiteOuvrir.addChoosableFileFilter(new Filtre(new String[]{"sec"}, "fichiers clés secrètes (.sec)"));
      boiteOuvrir.setFileHidingEnabled(false);
      boiteOuvrir.setAcceptAllFileFilterUsed(false);
      // Boite de sauvgarde.
      int choix = boiteOuvrir.showSaveDialog(boiteOuvrir);
        if (choix == boiteOuvrir.APPROVE_OPTION)
        {// Récupérer l'URL et le nom de fichier clé secrète
        String urlCléSecrète=boiteOuvrir.getSelectedFile().getPath();
        // Extension de la clé secrète.
        urlCléSecrète+=".sec";
           try
           {BufferedWriter out = new BufferedWriter(new FileWriter(urlCléSecrète));
           out.write(jTextField1.getText());
           out.close();
           // Effacer le contenu de jTextField1
           jTextField1.setText(null);
           }
           catch (IOException ex)
           {System.out.println("Erreur : "+ex);
           }
        }
      }
      else
      {JOptionPane jop=new JOptionPane();
      jop.showMessageDialog(null,"Générer la clé secrète.","Erreur",JOptionPane.ERROR_MESSAGE);
      }

    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
    int taille = 0;
    ClesRSA clesRsa = null;

    int i=jComboBox2.getSelectedIndex();
      switch(i)
      {  case 0:
         {taille=512;
          break;
         }
         case 1:
         {taille=1024;
          break;
         }
         case 2:
         {taille=2048;
          break;
         }
      }
      try
      {// Instanciation de l'objet clesRsa.
      clesRsa = new ClesRSA(taille);
      }
      catch (Exception ex)
      {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
      }
    module=clesRsa.getModule();
    exposantPublique=clesRsa.getExposantPublique();
    exposantPrive=clesRsa.getExposantPrive();

    jTextArea1.setText(Utils.alignerChaine(module.toString(),36));
    jTextArea2.setText(Utils.alignerChaine(exposantPrive.toString(), 36));
    jTextField2.setText(exposantPublique.toString());
    }//GEN-LAST:event_jButton3ActionPerformed

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
      if((jTextArea1.getText()!=null)&&(jTextArea1.getText().length()!=0))
      {JFileChooser boiteOuvrir = new JFileChooser();
      boiteOuvrir.setCurrentDirectory(new File("fichiers" + File.separator));
      boiteOuvrir.addChoosableFileFilter(new Filtre(new String[]{"pbl"}, "fichiers clés publiques (.pbl)"));
      boiteOuvrir.setFileHidingEnabled(false);
      boiteOuvrir.setAcceptAllFileFilterUsed(false);
      int choix = boiteOuvrir.showSaveDialog(boiteOuvrir);
        if (choix == boiteOuvrir.APPROVE_OPTION)
        {String urlClePublique=boiteOuvrir.getSelectedFile().getPath();
        // Extension de la clé publique.
        urlClePublique+=".pbl";
          try
          {BufferedWriter out = new BufferedWriter(new FileWriter(urlClePublique));
          out.write(module.toString());
          // Retour à la ligne.
          out.newLine();
          out.write(exposantPublique.toString());
          out.close();
          }
          catch (IOException ex)
          {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
          }
        }
      }
      else
      {JOptionPane jop=new JOptionPane();
      jop.showMessageDialog(null,"Générer les clés RSA.","Erreur",JOptionPane.ERROR_MESSAGE);
      }
    }//GEN-LAST:event_jButton4ActionPerformed

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed
      if((jTextArea2.getText()!=null)&&(jTextArea2.getText().length()!=0))
      {JFileChooser boiteOuvrir = new JFileChooser();
      boiteOuvrir.setCurrentDirectory(new File("fichiers" + File.separator));
      boiteOuvrir.addChoosableFileFilter(new Filtre(new String[]{"prv"}, "fichiers clés privées (.prv)"));
      boiteOuvrir.setFileHidingEnabled(false);
      boiteOuvrir.setAcceptAllFileFilterUsed(false);
      int choix = boiteOuvrir.showSaveDialog(boiteOuvrir);
        if (choix == boiteOuvrir.APPROVE_OPTION)
        {String urlClePrivee=boiteOuvrir.getSelectedFile().getPath();
        // Extension du fichier.
        urlClePrivee+=".prv";
          try
          {BufferedWriter out = new BufferedWriter(new FileWriter(urlClePrivee));
          out.write(module.toString());
          out.newLine();
          out.write(exposantPrive.toString());
          out.close();
          }
          catch (IOException ex)
          {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
          }
        }
      }
      else
      {JOptionPane jop=new JOptionPane();
      jop.showMessageDialog(null,"Générer les clés RSA.","Erreur",JOptionPane.ERROR_MESSAGE);
      }
    }//GEN-LAST:event_jButton5ActionPerformed

    private void jComboBox2ItemStateChanged(java.awt.event.ItemEvent evt) {//GEN-FIRST:event_jComboBox2ItemStateChanged
    jTextArea1.setText(null);
    jTextArea2.setText(null);
    jTextField2.setText(null);
    }//GEN-LAST:event_jComboBox2ItemStateChanged

    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton6ActionPerformed
    JFileChooser boiteOuvrir = new JFileChooser();
    boiteOuvrir.setCurrentDirectory(new File("fichiers" + File.separator));
    boiteOuvrir.addChoosableFileFilter(new Filtre(new String[]{"sec"}, "fichiers clés secrètes (.sec)"));
    boiteOuvrir.setFileHidingEnabled(false);
    boiteOuvrir.setAcceptAllFileFilterUsed(false);
    int choix = boiteOuvrir.showOpenDialog(boiteOuvrir);
      if (choix == boiteOuvrir.APPROVE_OPTION)
      {String urlCleSecrete=boiteOuvrir.getSelectedFile().getPath();
        try
        {BufferedReader fichier = new BufferedReader(new FileReader(urlCleSecrete));
        String cle=fichier.readLine();
        fichier.close();
        jTextField3.setText(cle);
        }
        catch (FileNotFoundException ex)
        {JOptionPane jop=new JOptionPane();
        jop.showMessageDialog(null,"Fichier non trouvé.","Erreur",JOptionPane.ERROR_MESSAGE);
        }
        catch (IOException ex)
        {Logger.getLogger(InterfacePrincipale.class.getName()).log(Level.SEVERE, null, ex);
        }
      }
    }//GEN-LAST:event_jButton6ActionPerformed

    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton7ActionPerformed
    JFileChooser boiteOuvrir = new JFileChooser();
    boiteOuvrir.setCurrentDirectory(new File("fichiers" + File.separator));
    boiteOuvrir.setFileHidingEnabled(false);
    boiteOuvrir.setAcceptAllFileFilterUsed(true);
    int choix = boiteOuvrir.showOpenDialog(boiteOuvrir);
       if (choix == boiteOuvrir.APPROVE_OPTION)
       {jTextField4.setText(boiteOuvrir.getSelectedFile().getPath());
       }
    }//GEN-LAST:event_jButton7ActionPerformed

    private void jButton8ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton8ActionPerformed
    t = new Thread(new ChiffrementSymetrique());
    t.start();
    }//GEN-LAST:event_jButton8ActionPerformed

